{
	"name": "kernelsquare-fe-admin",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"install": "husky install",
		"postinstall": "husky install",
		"lint": "eslint --cache \"src/**/*.{ts,tsx,js,jsx}\"",
		"lint:fix": "eslint --fix --cache \"src/**/*.{ts,tsx,js,jsx}\"",
		"formatter": "prettier --write --cache \"src/**/*.{ts,tsx,js,jsx}\""
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
